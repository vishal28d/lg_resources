<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2"
     xmlns:gx="http://www.google.com/kml/ext/2.2"
     xmlns:kml="http://www.opengis.net/kml/2.2"
     xmlns:atom="http://www.w3.org/2005/Atom">
  <Document>
    <name>House Model</name>
    <description>A 3D house model using COLLADA</description>
    
    <!-- Placemark for 3D Model -->
    <Placemark>
      <name>3D House</name>
      <description>This is a house model rendered using COLLADA (.dae)</description>
      <Model>
        <!-- Link to the COLLADA (.dae) file -->
        <Link>
          <href>https://raw.githubusercontent.com/vishal28d/task5/refs/heads/main/assets/house.dae</href>
        </Link>
        <!-- Position of the model -->
        <Location>
          <longitude>74.93528</longitude> <!-- Longitude -->
          <latitude>28.72056</latitude>   <!-- Latitude -->
          <altitude>0</altitude>          <!-- Altitude -->
        </Location>
        <!-- Orientation of the model -->
        <Orientation>
          <heading>0</heading>            <!-- Rotation in degrees -->
          <tilt>0</tilt>                  <!-- Tilt of the model -->
          <roll>0</roll>                  <!-- Roll of the model -->
        </Orientation>
        <!-- Scale of the model -->
        <Scale>
          <x>1</x>                        <!-- Scale factor for the X-axis -->
          <y>1</y>                        <!-- Scale factor for the Y-axis -->
          <z>1</z>                        <!-- Scale factor for the Z-axis -->
        </Scale>
        <!-- Altitude mode -->
        <altitudeMode>relativeToGround</altitudeMode>
      </Model>
    </Placemark>
  </Document>
</kml>
